{
  "name" : "kolapsis/androidgenerator-bundle",
  "minimum-stability": "dev",
  "description" : "Android Generator Bundle",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Benjamin Touchard",
    "email" : "benjamin@kolapsis.com"
  }],
  "keywords" : [
    "Android", "Generator", "Rest API"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=5.5.9",
    "symfony/framework-bundle": "~2.3|~3.0",
    "symfony/console": "~2.3|~3.0",
    "symfony/dependency-injection": "~2.3|~3.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "friendsofsymfony/user-bundle": "^2.0@dev",
    "friendsofsymfony/rest-bundle": "^2.1",
    "jms/serializer-bundle": "^1.1",
    "lexik/jwt-authentication-bundle": "^2.0"
  },
  "autoload" : {
    "psr-4" : {
      "Kolapsis\\Bundle\\AndroidGeneratorBundle\\" : ""
    }
  },
  "repositories" : [{
    "type" : "vcs",
    "url" : "https://github.com/btouchard/AndroidGeneratorBundle.git"
  }],
  "extra" : {
    "branch-alias" : {
      "dev-master" : "master-dev"
    }
  }
}